<script type="text/javascript" >
     $(function () {
         $('#dialog').dialog({
             autoOpen: false,
             width: 400,
             height: 500,
             resizable: false,
             title: 'Product',
             modal: true,
             open: function (event, ui) {
                 $(this).load("@Url.Action("ProductPartial", "JobTracking")");
            },
            buttons: {
                "Close": function () {
                    $(this).dialog("close");
                }
            }
        });
         $("#opener").click(function () {
             $("#dialog").dialog("open");
         });
     });
  </script>
<h2> Index</h2>
 
<div id="dialog" title="Basic dialog" >Please wait</div>
<button id="opener">Show Product</button>
 