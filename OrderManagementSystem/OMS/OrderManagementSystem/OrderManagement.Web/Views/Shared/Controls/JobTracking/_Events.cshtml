@model List<TrackingJobs>

<div class="targetDiv">
    <table class="table table-hover no-margins">
        <thead>
            <tr>
                <th>Status</th>
                <th>Address</th>
                <th>Product Group</th>
                <th>User</th>
                <th>Start time</th>
                <th>End Time</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count > 0)
            {


                foreach (var job in Model)
                {
                <tr>
                    <td><span class="label label-primary"><i class="fa fa-check"></i>@job.JobEventStatus</span></td>
                    <td>@job.JobTitle</td>
                    <td>@job.Product_Group</td>
                    <td>@job.Resource</td>
                    <td>@job.StartDate</td>
                    <td>@job.EndDate</td>
                   </tr>

                }
            }
            else
            {
                <span>No Record Found</span>
            }
