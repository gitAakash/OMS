@{
    Layout = null;
   
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    

    <title>Login</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")"  rel="stylesheet" />
    <link href="@Url.Content("~/Content/font-awesome.css")" rel="stylesheet"  />

    <link href="@Url.Content("~/Content/morris-0.4.3.min.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Content/jquery.gritter.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Content/animate.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Content/style.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    
    <!-- Mainly scripts -->
    <script src="@Url.Content("~/Scripts/Theme_Js/jquery-1.10.2.js")" ></script>
    <script src="@Url.Content("~/Scripts/Theme_Js/bootstrap.min.js")" ></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" ></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" ></script>
    <script src="@Url.Content("~/Scripts/jquery.capslockstate.js")"></script>

</head>

<body class="gray-bg">
    <div id="page">
        <div id="Logincontent">
            @if (TempData["message"] != null)
            {
                <div class="Message">@TempData["message"]</div>
            }
            @RenderBody()
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            //var Skin = "skin2"
           // $('body').addClass(Skin);
        });
    </script>
</body>
</html>
